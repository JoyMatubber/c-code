#include<iostream>
using namespace std;
int main()
{
cout<<"ENTER YOUR :"<<endl;
float number;
cin>>number;
if (number>=90 && number<=100)
{
cout<<"your grade is A+"<<endl;
}
else if (number>=85 && number<=89.99)
{
cout<<"your grade is A"<<endl;
}
else if (number>=80 && number<=84.99)
{
cout<<"your grade is B+"<<endl;
}
else if (number>=75 && number<=79.99)
{
cout<<"your grade is B"<<endl;
}
else if (number>=70 && number<=74.99)
{
cout<<"your grade is c+"<<endl;
}
else if (number>=65 && number<=69.99)
{
cout<<"your grade is c"<<endl;
}
else if (number>=60 && number<=64.99)
{
cout<<"your grade is D+"<<endl;
}
else if (number>=50 && number<=60)
{
cout<<"your grade is D"<<endl;
}
else if (number>=0 && number<=50)
{
cout<<"your grade is F"<<endl;
}
else
{
cout<<"NUMBER WHICH YOU PUT IS INAVLID"<<endl;
}
}
